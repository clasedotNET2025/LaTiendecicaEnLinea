{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ReverseProxy": {
    "Routes": {
      "identity-api-auth": {
        "ClusterId": "identity-api",
        "Match": {
          "Path": "/api/v{version}/auth/{**catch-all}"
        }
      },
      "identity-api-users": {
        "ClusterId": "identity-api",
        "Match": {
          "Path": "/api/v{version}/users/{**catch-all}"
        }
      },
      "catalog-categories": {
        "ClusterId": "catalog-api",
        "Match": {
          "Path": "/api/v1/categories/{**catch-all}"
        }
      },
      "catalog-products": {
        "ClusterId": "catalog-api",
        "Match": {
          "Path": "/api/v1/products/{**catch-all}"
        }
      }
    },
    "Clusters": {
      "identity-api": {
        "Destinations": {
          "destination1": {
            "Address": "http://localhost:59331"
          }
        }
      },
      "catalog-api": {
        "Destinations": {
          "destination1": {
            "Address": "http://localhost:59365"
          }
        }
      }
    }
  }
}